{
  "extends": "@adonisjs/tsconfig/tsconfig.app.json",
  "compilerOptions": {
    "rootDir": "./",
    "outDir": "./build",
    "paths": {
      "#exceptions/*": [
        "./app/exceptions/*.js"
      ],
      "#models/abstracts": [
        "./app/models/abstracts/index.js"
      ],
      "#models": [
        "./app/models/index.js"
      ],
      "#models/*": [
        "./app/models/*.js"
      ],
      "#services/*": [
        "./app/services/*.js"
      ],
      "#validators/*": [
        "./app/validators/*.js"
      ],
      "#providers/*": [
        "./providers/*.js"
      ],
      "#database/*": [
        "./database/*.js"
      ],
      "#start/*": [
        "./start/*.js"
      ],
      "#config/*": [
        "./config/*.js"
      ]
    }
  }
}